    "def extract_review_data(review_element):\n",
    "    \"\"\"Extrae información detallada de un elemento de reseña.\"\"\"\n",
    "    review_data = {}\n",
    "    \n",
    "    try:\n",
    "        # Extraer nombre del revisor y país\n",
    "        name_elements = review_element.find_elements(By.CSS_SELECTOR, \n",
    "            '.c-reviewer-details h4, .review_list_reviewer_name')\n",
    "        if name_elements:\n",
    "            review_data['reviewer_name'] = name_elements[0].text.strip()\n",
    "        \n",
    "        country_elements = review_element.find_elements(By.CSS_SELECTOR, \n",
    "            '.bui-avatar-block__subtitle, span[data-testid=\"country-flag\"]')\n",
    "        if country_elements:\n",
    "            review_data['reviewer_country'] = country_elements[0].text.strip()\n",
    "        \n",
    "        # Extraer fecha de la reseña\n",
    "        date_elements = review_element.find_elements(By.CSS_SELECTOR, \n",
    "            '.review_item_date, .review_list_date, [data-testid=\"review-date\"]')\n",
    "        if date_elements:\n",
    "            review_data['date'] = date_elements[0].text.strip()\n",
    "        \n",
    "        # Extraer puntuación de la reseña\n",
    "        score_elements = review_element.find_elements(By.CSS_SELECTOR, \n",
    "            '.bui-review-score__badge, .review-score-badge, [data-testid=\"review-score\"]')\n",
    "        if score_elements:\n",
    "            score_text = score_elements[0].text.strip()\n",
    "            score_match = re.search(r'(\\d+(\\.\\d+)?)', score_text)\n",
    "            if score_match:\n",
    "                review_data['score'] = score_match.group(1)\n",
    "            else:\n",
    "                review_data['score'] = score_text\n",
    "        \n",
    "        # Extraer título de la reseña\n",
    "        title_elements = review_element.find_elements(By.CSS_SELECTOR, \n",
    "            '.c-review-block__title, .review_item_header_content, [data-testid=\"review-title\"]')\n",
    "        if title_elements:\n",
    "            review_data['title'] = title_elements[0].text.strip()\n",
    "        \n",
    "        # Extraer comentario positivo\n",
    "        positive_elements = review_element.find_elements(By.CSS_SELECTOR, \n",
    "            '.review_pos, .c-review__positive, [data-testid=\"review-positive\"]')\n",
    "        if positive_elements:\n",
    "            review_data['positive'] = positive_elements[0].text.replace('좋았던 점:', '').replace('Me gustó:', '').replace('Lo que me gustó:', '').strip()\n",
    "        \n",
    "        # Extraer comentario negativo\n",
    "        negative_elements = review_element.find_elements(By.CSS_SELECTOR, \n",
    "            '.review_neg, .c-review__negative, [data-testid=\"review-negative\"]')\n",
    "        if negative_elements:\n",
    "            review_data['negative'] = negative_elements[0].text.replace('아쉬웠던 점:', '').replace('No me gustó:', '').replace('Lo que no me gustó:', '').strip()\n",
    "        \n",
    "        # Extraer detalles de la estancia (tipo de habitación, noches)\n",
    "        room_elements = review_element.find_elements(By.CSS_SELECTOR, \n",
    "            '.review_staydate, .stay-details__room, [data-testid=\"review-room-info\"]')\n",
    "        if room_elements:\n",
    "            review_data['room_info'] = room_elements[0].text.strip()\n",
    "        \n",
    "        nights_elements = review_element.find_elements(By.CSS_SELECTOR, \n",
    "            '.review_staydate, .stay-details__nights, [data-testid=\"review-stay-info\"]')\n",
    "        if nights_elements:\n",
    "            stay_text = nights_elements[0].text.strip()\n",
    "            nights_match = re.search(r'(\\d+)\\s+(noche|night|noite)', stay_text, re.IGNORECASE)\n",
    "            if nights_match:\n",
    "                review_data['nights'] = nights_match.group(1)\n",
    "            else:\n",
    "                review_data['stay_info'] = stay_text\n",
    "        \n",
    "        # Obtener contenido completo en bruto\n",
    "        review_data['content'] = review_element.text.strip()\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"Error al extraer detalles de la reseña: {e}\")\n",
    "    \n",
    "    return review_data\n",
    "\n",
    "def create_output_dir():\n",
    "    \"\"\"Crea el directorio de salida para los archivos JSON si no existe.\"\"\"\n",
    "    output_dir = \"booking_scraper_output\"\n",
    "    if not os.path.exists(output_dir):\n",
    "        os.makedirs(output_dir)\n",
    "    return output_dir\n",
    "\n",
    "def handle_popups(driver):\n",
    "    \"\"\"Maneja los pop-ups de cookies y otros elementos superpuestos.\"\"\"\n",
    "    try:\n",
    "        # Consentimiento de cookies\n",
    "        cookie_selectors = [\n",
    "            \"#onetrust-accept-btn-handler\",\n",
    "            \"button[id*='cookie'], button[id*='Cookie']\",\n",
    "            \"button[aria-label*='cookie'], button[aria-label*='Cookie']\",\n",
    "            \"button:contains('Accept')\"\n",
    "        ]\n",
    "        \n",
    "        for selector in cookie_selectors:\n",
    "            try:\n",
    "                if selector.startswith(\"button:contains\"):\n",
    "                    # Manejar sintaxis tipo jQuery\n",
    "                    text = selector.split(\"'\")[1]\n",
    "                    button = driver.find_element(By.XPATH, f\"//button[contains(text(), '{text}')]\")\n",
    "                else:\n",
    "                    button = driver.find_element(By.CSS_SELECTOR, selector)\n",
    "                button.click()\n",
    "                print(f\"Botón de cookies clickeado usando {selector}\")\n",
    "                time.sleep(1)\n",
    "                break\n",
    "            except (NoSuchElementException, TimeoutException):\n",
    "                continue\n",
    "        \n",
    "        # Manejar otros popups comunes (newsletter, descarga de app, etc.)\n",
    "        popup_close_selectors = [\n",
    "            \"button[aria-label='Close'], button.close-button, button.modal__close\",\n",
    "            \"button.modal__close, div.modal__close\",\n",
    "            \"svg[aria-label='Close']\",\n",
    "            \"div[role='dialog'] button\"\n",
    "        ]\n",
    "        \n",
    "        for selector in popup_close_selectors:\n",
    "            try:\n",
    "                close_buttons = driver.find_elements(By.CSS_SELECTOR, selector)\n",
    "                for button in close_buttons:\n",
    "                    if button.is_displayed():\n",
    "                        button.click()\n",
    "                        print(f\"Popup cerrado usando {selector}\")\n",
    "                        time.sleep(1)\n",
    "            except Exception:\n",
    "                pass\n",
    "                \n",
    "    except Exception as e:\n",
    "        print(f\"Error manejando popups: {e}\")\n",
    "\n",
    "def navigate_to_next_page(driver, current_page):\n",
    "    \"\"\"Navega a la siguiente página de reseñas modificando la URL.\"\"\"\n",
    "    try:\n",
    "        print(f\"\\nIntentando navegar a la página {current_page + 1}...\")\n",
    "        \n",
    "        current_url = driver.current_url\n",
    "        print(f\"URL actual: {current_url}\")\n",
    "        \n",
    "        # Estrategia principal: Modificar la URL directamente\n",
    "        if 'page=' in current_url:\n",
    "            # Reemplazar el número de página existente\n",
    "            new_url = re.sub(r'page=\\d+', f'page={current_page + 1}', current_url)\n",
    "        else:\n",
    "            # Si no hay parámetro de página, agregarlo\n",
    "            if '?' in current_url:\n",
    "                new_url = f\"{current_url}&page={current_page + 1}\"\n",
    "            else:\n",
    "                new_url = f\"{current_url}?page={current_page + 1}\"\n",
    "        \n",
    "        print(f\"Navegando a URL construida: {new_url}\")\n",
    "        driver.get(new_url)\n",
    "        time.sleep(3)\n",
    "        \n",
    "        # Verificar si hubo redirección a una página diferente (lo que podría indicar final de resultados)\n",
    "        if driver.current_url != new_url:\n",
    "            print(\"Posible redirección detectada. URL actual después de navegación: \" + driver.current_url)\n",
    "            \n",
    "            # Si se redirecciona a la URL base, podría indicar que no hay más páginas\n",
    "            base_part = current_url.split('?')[0]\n",
    "            if driver.current_url == base_part:\n",
    "                print(\"Redirección a URL base detectada. Probablemente no hay más páginas.\")\n",
    "                return False\n",
    "        \n",
    "        # Verificar si la página tiene contenido de reseñas\n",
    "        review_containers = driver.find_elements(By.CSS_SELECTOR, \n",
    "            \".review_list_new_item_block, .review_item, [data-testid='review-card'], .c-review-block\")\n",
    "        \n",
    "        if not review_containers:\n",
    "            print(\"No se encontraron reseñas en la nueva página. Posiblemente final de resultados.\")\n",
    "            return False\n",
    "            \n",
    "        print(f\"Navegación exitosa a página {current_page + 1} con {len(review_containers)} reseñas encontradas\")\n",
    "        return True\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"Error navegando a la siguiente página: {e}\")\n",
    "        return False\n",
    "\n",
    "def main():\n",
    "    output_dir = create_output_dir()\n",
    "    \n",
    "    # Configurar opciones de Chrome con ajustes para evitar detección\n",
    "    options = webdriver.ChromeOptions()\n",
    "    options.add_argument(\"--start-maximized\")\n",
    "    options.add_argument(\"--disable-notifications\")\n",
    "    options.add_argument(\"--disable-popup-blocking\")\n",
    "    options.add_argument(\"--disable-blink-features=AutomationControlled\")\n",
    "    options.add_experimental_option(\"excludeSwitches\", [\"enable-automation\"])\n",
    "    options.add_experimental_option(\"useAutomationExtension\", False)\n",
    "    # Usar un agente de usuario realista\n",
    "    options.add_argument(\"user-agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/122.0.0.0 Safari/537.36\")\n",
    "    \n",
    "    driver = None\n",
    "    all_reviews = []\n",
    "    \n",
    "    try:\n",
    "        driver = webdriver.Chrome(service=Service(ChromeDriverManager().install()), options=options)\n",
    "        \n",
    "        # Ejecutar scripts para dificultar la detección de bots\n",
    "        driver.execute_script(\"Object.defineProperty(navigator, 'webdriver', {get: () => undefined})\")\n",
    "        \n",
    "        # URL directa a la sección de reseñas de la página del hotel\n",
    "        url = \"https://www.booking.com/reviews/co/hotel/absolute-amp-hostel-boutique.es.html\"\n",
    "        \n",
    "        print(\"Navegando a URL de reseñas...\")\n",
    "        driver.get(url)\n",
    "        \n",
    "        # Esperar a que la página cargue\n",
    "        time.sleep(5)\n",
    "        \n",
    "        # Manejar consentimiento de cookies y otros popups\n",
    "        handle_popups(driver)\n",
    "        \n",
    "        # Inicializar contador de página y máximo de páginas\n",
    "        current_page = 1\n",
    "        max_pages = 50 # Ajustar según necesidad\n",
    "        reviews_per_page = []\n",
    "        \n",
    "        # Guardar la URL de la primera página para referencia\n",
    "        first_page_url = driver.current_url\n",
    "        print(f\"URL de primera página: {first_page_url}\")\n",
    "        \n",
    "        while current_page <= max_pages:\n",
    "            print(f\"\\n--- Procesando Página de Reseñas {current_page} de máximo {max_pages} ---\")\n",
    "            \n",
    "            # Dar tiempo a la página para cargar completamente\n",
    "            time.sleep(3)\n",
    "            \n",
    "            # Manejar cualquier popup que pueda reaparecer\n",
    "            handle_popups(driver)\n",
    "            \n",
    "            # Desplazarse gradualmente hacia abajo para asegurar que todas las reseñas están cargadas\n",
    "            total_height = driver.execute_script(\"return document.body.scrollHeight\")\n",
    "            for i in range(1, 5): # Desplazarse en 4 pasos\n",
    "                driver.execute_script(f\"window.scrollTo(0, {total_height * i / 4});\")\n",
    "                time.sleep(0.5)\n",
    "            \n",
    "            # Probar diferentes selectores de contenedor de reseñas\n",
    "            review_selector_options = [\n",
    "                \".review_list_new_item_block\",\n",
    "                \".review_item\",\n",
    "                \".c-review-block\",\n",
    "                \".review_list .review_item\",\n",
    "                \"[data-testid='review-card']\",\n",
    "                \".review-list .review-list-new__container .review-list-new__item .review-list-new-component\",\n",
    "                \".review_item_review\",\n",
    "                \".review-score-widget\"\n",
    "            ]\n",
    "            \n",
    "            review_elements = []\n",
    "            for selector in review_selector_options:\n",
    "                try:\n",
    "                    elements = WebDriverWait(driver, 5).until(\n",
    "                        EC.presence_of_all_elements_located((By.CSS_SELECTOR, selector))\n",
    "                    )\n",
    "                    if elements:\n",
    "                        review_elements = elements\n",
    "                        print(f\"Encontradas {len(elements)} reseñas usando selector: {selector}\")\n",
    "                        break\n",
    "                except (TimeoutException, NoSuchElementException):\n",
    "                    continue\n",
    "            \n",
    "            if not review_elements:\n",
    "                print(\"No se encontraron reseñas en la página actual\")\n",
    "                # Guardar HTML para depuración\n",
    "                with open(f\"{output_dir}/reviews_page_{current_page}_source.html\", \"w\", encoding=\"utf-8\") as f:\n",
    "                    f.write(driver.page_source)\n",
    "                break\n",
    "            \n",
    "            # Registrar recuento de reseñas para esta página\n",
    "            reviews_per_page.append(len(review_elements))\n",
    "            print(f\"Encontradas {len(review_elements)} reseñas en la página {current_page}\")\n",
    "            \n",
    "            # Procesar reseñas en esta página\n",
    "            new_reviews_count = 0\n",
    "            for i, element in enumerate(review_elements):\n",
    "                try:\n",
    "                    driver.execute_script(\"arguments[0].scrollIntoView({block: 'center'});\", element)\n",
    "                    time.sleep(0.3) # Breve pausa para asegurar que el elemento esté completamente cargado\n",
    "                    \n",
    "                    review_data = extract_review_data(element)\n",
    "                    review_data['page'] = current_page\n",
    "                    review_data['position'] = i + 1\n",
    "                    all_reviews.append(review_data)\n",
    "                    new_reviews_count += 1\n",
    "                    print(f\"Procesada reseña #{len(all_reviews)} (página {current_page}, elemento {i+1})\")\n",
    "                except StaleElementReferenceException:\n",
    "                    print(f\"El elemento se volvió obsoleto durante el procesamiento (página {current_page}, elemento {i+1})\")\n",
    "                except Exception as e:\n",
    "                    print(f\"Error procesando reseña: {e}\")\n",
    "            \n",
    "            # Verificar si realmente obtuvimos nuevas reseñas en esta página\n",
    "            if new_reviews_count == 0:\n",
    "                print(\"No se extrajeron nuevas reseñas en esta página. Podría ser el final.\")\n",
    "                break\n",
    "            \n",
    "            # Guardar progreso después de cada página\n",
    "            with open(f\"{output_dir}/booking_reviews_page_{current_page}.json\", \"w\", encoding=\"utf-8\") as f:\n",
    "                json.dump(all_reviews, f, ensure_ascii=False, indent=2)\n",
    "            \n",
    "            # Verificar si debemos continuar a la siguiente página\n",
    "            if current_page >= max_pages:\n",
    "                print(f\"Se alcanzó el máximo de páginas ({max_pages}). Deteniendo.\")\n",
    "                break\n",
    "            \n",
    "            # Verificar si hemos llegado a la última página\n",
    "            if len(reviews_per_page) >= 2:\n",
    "                if reviews_per_page[-1] == 0:\n",
    "                    print(\"No hay reseñas en la página actual. Probablemente se llegó al final.\")\n",
    "                    break\n",
    "            \n",
    "            # Esperar un poco más antes de intentar navegar\n",
    "            time.sleep(2)\n",
    "            \n",
    "            # Intentar navegar a la siguiente página con función mejorada\n",
    "            next_page_success = navigate_to_next_page(driver, current_page)\n",
    "            if next_page_success:\n",
    "                current_page += 1\n",
    "                \n",
    "                # Agregar retraso aleatorio entre navegación de páginas (ayuda a evitar detección)\n",
    "                delay = random.uniform(3.5, 6.0)\n",
    "                print(f\"Esperando {delay:.2f} segundos antes de procesar la siguiente página...\")\n",
    "                time.sleep(delay)\n",
    "            else:\n",
    "                print(\"No se pudo navegar a la siguiente página. Rompiendo el bucle.\")\n",
    "                break\n",
    "    \n",
    "    # Guardar todas las reseñas recopiladas en JSON\n",
    "    with open(f\"{output_dir}/booking_reviews_all.json\", \"w\", encoding=\"utf-8\") as f:\n",
    "        json.dump(all_reviews, f, ensure_ascii=False, indent=2)\n",
    "    \n",
    "    print(f\"\\n¡Scraping completado!\")\n",
    "    print(f\"Total de reseñas recopiladas: {len(all_reviews)}\")\n",
    "    print(f\"Total de páginas procesadas: {current_page}\")\n",
    "    \n",
    "    except Exception as e:\n",
    "        print(f\"Ocurrió un error: {e}\")\n",
    "        \n",
    "        # Guardar cualquier reseña recopilada en caso de error\n",
    "        if all_reviews:\n",
    "            with open(f\"{output_dir}/booking_reviews_partial.json\", \"w\", encoding=\"utf-8\") as f:\n",
    "                json.dump(all_reviews, f, ensure_ascii=False, indent=2)\n",
    "            print(f\"Guardadas {len(all_reviews)} reseñas en booking_reviews_partial.json\")\n",
    "            \n",
    "    finally:\n",
    "        # Cerrar el navegador\n",
    "        if driver:\n",
    "            driver.quit()\n",
    "            print(\"Navegador cerrado con éxito\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
